<%= form_for(event) do |f| %>
    <%= f.label :title %><br />
    <%= f.text_field :title %><br />

    <%= f.label :start_date %><br />
    <%= f.datetime_field :start_date %><br />

    <%= f.label :end_date %><br />
    <%= f.datetime_field :end_date %><br />

    <%= f.label :online 'Is the event online?' %><br />

    <%= f.label :online_link 'URL to join the online event:' %><br />
    <%= f.url_field :online_link %><br />

    <%= f.label :description %><br />
    <%= f.text_area :description %><br />

    <%= f.submit 'Submit' %>

<% end %>
